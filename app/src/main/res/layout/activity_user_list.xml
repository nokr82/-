<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_gradient"
    android:orientation="horizontal">
    <com.devstories.anipointcompany.android.base.RationalLinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:ratio="0:360"
        app:adjustHeight="false">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">


    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="16dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginRight="9.5dp"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="155dp"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/userLL"
                android:layout_width="140dp"
                android:layout_height="40dp"
                android:background="@drawable/background_strock_707070"
                android:gravity="center">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:lineSpacingExtra="25sp"
                    android:text="고객목록"
                    android:textColor="#ffffff"
                    android:textSize="20dp" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/pointLL"
                android:layout_width="130dp"
                android:layout_height="40dp"
                android:layout_marginLeft="5dp"
                android:gravity="center">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    android:lineSpacingExtra="25sp"
                    android:text="포인트 내역"
                    android:textColor="#ffffff"
                    android:textSize="20dp" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/uservisitLL"
                android:layout_width="140dp"
                android:layout_height="40dp"
                android:layout_marginLeft="5dp"
                android:gravity="center">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:lineSpacingExtra="25sp"
                    android:text="고객 방문 분석"
                    android:textColor="#ffffff"
                    android:textSize="20dp" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/analysisRevenueLL"
                android:layout_width="120dp"
                android:layout_height="40dp"
                android:layout_marginLeft="5dp"
                android:gravity="center">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:lineSpacingExtra="25sp"
                    android:text="매출분석"
                    android:textColor="#ffffff"
                    android:textSize="20dp" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/messageLL"
                android:layout_width="170dp"
                android:layout_height="40dp"
                android:layout_marginLeft="5dp"
                android:gravity="center">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:lineSpacingExtra="25sp"
                    android:text="메세지/쿠폰관리"
                    android:textColor="#ffffff"
                    android:textSize="20dp" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/settingLL"
                android:layout_width="120dp"
                android:layout_height="40dp"
                android:layout_marginLeft="5dp"
                android:gravity="center">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:lineSpacingExtra="25sp"
                    android:text="설정"
                    android:textColor="#ffffff"
                    android:textSize="20dp" />
            </LinearLayout>


        </LinearLayout>

        <com.devstories.anipointcompany.android.base.RationalRelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:ratio="550:360">
        <FrameLayout
            android:id="@+id/userFL"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="10dp">

        </FrameLayout>
        </com.devstories.anipointcompany.android.base.RationalRelativeLayout>




    </LinearLayout>

    
    </com.devstories.anipointcompany.android.base.RationalLinearLayout>




    
    
</LinearLayout>